<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/et_sets"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_menu_edit" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageButton"
            android:layout_marginTop="-8dp"
            app:srcCompat="@android:drawable/ic_menu_edit" />

        <EditText
            android:id="@+id/et_symbol_reps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="51dp"
            android:layout_toEndOf="@+id/imageButton"
            android:ems="10"
            android:hint="Repetitions (First Set)"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/et_symbol_reps2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="51dp"
            android:layout_toEndOf="@+id/imageButton"
            android:ems="10"
            android:hint="Repetitions (First Set)"
            android:inputType="textPersonName"
            android:layout_below="@+id/et_symbol_reps"/>

        <Button
            android:id="@+id/btn_upload_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_symbol_reps2"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="90dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="90dp"
            android:text="Button"
            />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>